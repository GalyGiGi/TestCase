<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:circlepro="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:gravity="center"
    android:orientation="vertical">

    <com.example.zengcheng.myfirstandroid.view.CircleProgressView
        android:layout_width="300dp"
        android:layout_height="500dp"
        circlepro:back_color="#232426"
        circlepro:back_line_color="#90ffffff"
        circlepro:circle_width="@dimen/circle_progress_line_width"
        circlepro:line_progress_color="#05c7c0"
        circlepro:progress="90" />
</LinearLayout>